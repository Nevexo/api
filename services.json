{
  "services": [
    {
      "name": "Twitter",
      "slug": "twitter",
      "endpoints": {
        "baseUrl": "https://www.twitter.com",
        "profileUrl": "https://twitter.com/%name%",
        "requestUrl": "https://api.twitter.com/i/users/username_available.json?username=%name%"
      },
      "httpMethod": "GET",
      "usernameCriteria": {
        "minLength": 5,
        "maxLength": 15,
        "allowedUsernamePattern": "([a-zA-Z0-9_]+)",
        "prohibitedWords": [
          "twitter",
          "admin"
        ]
      }
    },
